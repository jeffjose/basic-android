import androidx.compose.material3.Text
import androidx.compose.material3.Button
import androidx.compose.runtime.mutableStateMapOf


var $count = 0
val $user = mutableStateMapOf("loggedIn" to false)
println(user)

Button(onClick={ 
  user["loggedIn"] = true
  }){
  Text(text="Log in ")
}


Button(onClick={ 
  user["loggedIn"] = false
  }){
  Text(text="Log out ")
}

Text(text="${user.getValue("loggedIn")}")
